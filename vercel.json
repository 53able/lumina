{
  "buildCommand": "pnpm build && tsc -p tsconfig.api.json",
  "outputDirectory": "dist",
  "functions": {
    "api/index.ts": {
      "includeFiles": "src/**/*.js"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/health",
      "destination": "/api"
    }
  ]
}
